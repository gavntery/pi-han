   <package fileName="20200124-1-script.zip" sha1="1ac148306318b6928e6f20e9ed67b762a9257bbe" type="script" repo="https://pixinsight.com/update/1.8.8-4/" releaseDate="20200124" installDate="20200124201056" remove="src/scripts/PIDocCompiler" title="Documentation Compiler script version 1.6.5 (update/bugfix)">
